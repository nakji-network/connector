fmt := import("fmt")
exch := import("exchange")
t := import("times")
mfi := import("indicator/mfi")

load := func() {
    start := t.date(2017, 8 , 17 , 0 , 0 , 0, 0)
    end := t.add_date(start, 0, 6 , 0)
    ohlcvData := exch.ohlcv("binance", "BTC-USDT", "-", "SPOT", start, end, "1d")

    ret := mfi.calculate(ohlcvData.candles, 14)
    fmt.println(ret)
}

load()
